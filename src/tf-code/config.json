{
  "data": {
    "dataset_host": "https://itsci.mju.ac.th/downloads/watcharin/datasets/pv/",
    "dataset_file": "export_device_1_basic_aggregated_15minutes.csv.gz",
    "all_cols": [
      "Grid Feed In",
      "External Energy Supply", 
      "Internal Power Supply",
      "Current Power",
      "Self Consumption",
      "Ambient Temperature",
      "Module Temperature",
      "Total Irradiation"
    ],
    "train_val_test_split": [0.7, 0.2, 0.1]
  },
  "window": {
    "input_width": 24,
    "label_width": 1,
    "shift": 1,
    "label_columns": ["Current Power"]
  },
  "frequency_analysis": {
    "enabled": true,
    "target_column": "Current Power",
    "sample_period": 900
  },
  "models": [
    {
      "name": "svm",
      "title": "Support Vector Machine",
      "type": "svm",
      "enabled": true,
      "config": {
        "kernel": "rbf",
        "C": 1.0,
        "epsilon": 0.1,
        "gamma": "scale",
        "dev_mode": true,
        "dev_sample_ratio": 0.1,
        "checkpoint_enabled": true,
        "load_existing_model": true,
        "optimization_method": "grid",
        "optimization_params": {
          "C": [0.1, 1.0, 10.0],
          "epsilon": [0.01, 0.1, 0.2],
          "gamma": ["scale", "auto"]
        },
        "random_state": 42
      }
    },
    {
      "name": "tensorflow",
      "title": "TensorFlow Neural Network", 
      "type": "tensorflow",
      "enabled": false,
      "config": {
        "max_epochs": 20,
        "patience": 2,
        "batch_size": 32,
        "learning_rate": 0.001
      }
    },
    {
      "name": "adaboost",
      "title": "AdaBoost Classifier",
      "type": "adaboost", 
      "enabled": true,
      "config": {
        "n_estimators": 20,
        "learning_rate": 1.0,
        "differencing_order": 1,
        "dev_mode": true,
        "dev_sample_ratio": 0.1,
        "checkpoint_enabled": true,
        "load_existing_model": true,
        "random_state": 42
      }
    }
  ],
  "system": {
    "log_level": "INFO",
    "results_dir": "results",
    "checkpoint_dir": "checkpoints", 
    "enable_visualization": true,
    "max_training_epochs": 5
  }
}
